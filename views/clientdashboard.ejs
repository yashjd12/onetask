<!doctype html>
<html lang="en">
 
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">

    <link rel="stylesheet" href = "/ratings.css">
    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="clientdashboard/assets/vendor/bootstrap/css/bootstrap.min.css">
    <link href="clientdashboard/assets/vendor/fonts/circular-std/style.css" rel="stylesheet">
    <link rel="stylesheet" href="clientdashboard/assets/libs/css/style.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/fonts/fontawesome/css/fontawesome-all.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/charts/chartist-bundle/chartist.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/charts/morris-bundle/morris.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/fonts/material-design-iconic-font/css/materialdesignicons.min.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/charts/c3charts/c3.css">
    <link rel="stylesheet" href="clientdashboard/assets/vendor/fonts/flag-icon-css/flag-icon.min.css">
    <title>Client-Dashboard</title>
</head>

<body>
    <!-- ============================================================== -->
    <!-- main wrapper -->
    <!-- ============================================================== -->
    <div class="dashboard-main-wrapper">
        <!-- ============================================================== -->
        <!-- navbar -->
        <!-- ============================================================== -->
        <div class="dashboard-header">
            <nav class="navbar navbar-expand-lg bg-white fixed-top">
                <a class="navbar-brand" href="/client-dashboard">Onetask</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <form action="/">
                    <button type = "submit">Logout</button>
                </form>
            </nav>
        </div>
        <!-- ============================================================== -->
        <!-- end navbar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- left sidebar -->
        <!-- ============================================================== -->
        <div class="nav-left-sidebar sidebar-dark">
            <div class="menu-list">
                <nav class="navbar navbar-expand-lg navbar-light">
                    <a class="d-xl-none d-lg-none" href="#">Dashboard</a>
                    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="navbar-toggler-icon"></span>
                    </button>
                    <div class="collapse navbar-collapse" id="navbarNav">
                        <ul class="navbar-nav flex-column">
                            <li class="nav-divider">
                                Menu
                            </li>
                            <li class="nav-item ">
                                <a class="nav-link active" href="#" data-toggle="collapse" aria-expanded="false" data-target="#submenu-1" aria-controls="submenu-1"><i class="fa fa-fw fa-user-circle"></i>Dashboard <span class="badge badge-success">6</span></a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/client-previous-tasks" data-toggle="collapse" aria-expanded="false" data-target="#submenu-2" aria-controls="submenu-2"><i class="fa fa-fw fa-rocket"></i>Previous Tasks</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link" href="/task" data-toggle="collapse" aria-expanded="false" data-target="#submenu-5" aria-controls="submenu-5"><i class="fas fa-fw fa-table"></i>Book Another Service</a>
                            </li>
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end left sidebar -->
        <!-- ============================================================== -->
        <!-- ============================================================== -->
        <!-- wrapper  -->
        <!-- ============================================================== -->
        <div class="dashboard-wrapper">
            <div class="dashboard-ecommerce">
                <div class="container-fluid dashboard-content ">
                    <!-- ============================================================== -->
                    <!-- pageheader  -->
                    <!-- ============================================================== -->
                    <div class="row">
                        <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                            <div class="page-header">
                                <h2 class="pageheader-title">Client Dashboard</h2>
                            </div>
                        </div>
                    </div>
                    <!-- ============================================================== -->
                    <!-- end pageheader  -->
                    <!-- ============================================================== -->
                    <div class="ecommerce-widget">
                        <div class="row">
                            <!-- ============================================================== -->
                      
                            <!-- ============================================================== -->

                                          <!-- recent orders  -->
                            <!-- ============================================================== -->
                            <div class="col-xl-9 col-lg-12 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                    <h5 class="card-header">Recent Orders</h5>
                                    <div class="card-body p-0">
                                        <div class="table-responsive">
                                            <table class="table">
                                                <thead class="bg-light">
                                                    <tr class="border-0">
                                                        <th class="border-0">Taskname</th> 
                                                        <th class="border-0">Task Date</th>       
                                                        <th class="border-0">Employee Firstname</th>
                                                        <th class="border-0">Employee Lastname</th>
                                                        <th class="border-0">Employee Contact</th>
                                                        <th class="border-0">Employee Average Ratings</th>
                                                        <th class="border-0">Rate</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% for(var i=0; i<items.length; i++) {%>
                                                        <tr>
                                                            <td><%=items[i].Tname%></td>
                                                            <td><%=items[i].Tdate%></td>
                                                            <td><%=items[i].Fname%></td>
                                                            <td><%=items[i].Lname%></td>
                                                            <td><%=items[i].Contact%></td>
                                                            <td><%=avgratings[0].r%></td>
                                                            <td><div id="wrapper">
                                                                <form action="" method="post">
                                                                  <p class="clasificacion">
                                                                     <input id="radio1" type="radio" name="estrellas" value="5"><!--
                                                                    --><label for="radio1">&#9733;</label><!--
                                                                    --><input id="radio2" type="radio" name="estrellas" value="4"><!--
                                                                    --><label for="radio2">&#9733;</label><!--
                                                                    --><input id="radio3" type="radio" name="estrellas" value="3"><!--
                                                                    --><label for="radio3">&#9733;</label><!--
                                                                    --><input id="radio4" type="radio" name="estrellas" value="2"><!--
                                                                    --><label for="radio4">&#9733;</label><!--
                                                                    --><input id="radio5" type="radio" name="estrellas" value="1"><!--
                                                                    --><label for="radio5">&#9733;</label>
                                                                  </p>
                                                                  <p>
                                                                    <input type="submit" value="submit" name="submit" />
                                                                  </p>
                                                                </form>
                                                              </div>
                                                              <div>
                                                              
                                                              </div>
                                                            </td>
                                                            <td><span class="badge-dot badge-brand mr-1"></span>InTransit </td>
                                                        </tr>
                                                     <% } %>
                                                    
                                                    
                                                    
                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                            <!-- ============================================================== -->
                            <!-- end recent orders  -->

    
                            <!-- ============================================================== -->
                            <!-- ============================================================== -->
                            <!-- customer acquistion  -->
                            <!-- ============================================================== -->
                            <div class="col-xl-3 col-lg-6 col-md-6 col-sm-12 col-12">
                                <div class="card">
                                    <h5 class="card-header">Customer Acquisition</h5>
                                    <div class="card-body">
                                        <div class="ct-chart ct-golden-section" style="height: 354px;"></div>
                                        <div class="text-center">
                                            <span class="legend-item mr-2">
                                                    <span class="fa-xs text-primary mr-1 legend-tile"><i class="fa fa-fw fa-square-full"></i></span>
                                            <span class="legend-text">Returning</span>
                                            </span>
                                            <span class="legend-item mr-2">

                                                    <span class="fa-xs text-secondary mr-1 legend-tile"><i class="fa fa-fw fa-square-full"></i></span>
                                            <span class="legend-text">First Time</span>
                                            </span>
                                        </div>
                                    </div>
                                </div>
                            </div>
                           
                        


                        
                        
                    </div>
                </div>
            </div>
            <!-- ============================================================== -->
            <!-- footer -->
            <!-- ============================================================== -->
            <!-- ============================================================== -->
            <!-- end footer -->
            <!-- ============================================================== -->
        </div>
        <!-- ============================================================== -->
        <!-- end wrapper  -->
        <!-- ============================================================== -->
    </div>
    <!-- ============================================================== -->
    <!-- end main wrapper  -->
    <!-- ============================================================== -->
    <!-- Optional JavaScript -->
    <!-- jquery 3.3.1 -->
    <script src="clientdashboard/clientdashboard/assets/vendor/jquery/jquery-3.3.1.min.js"></script>
    <!-- bootstap bundle js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/bootstrap/js/bootstrap.bundle.js"></script>
    <!-- slimscroll js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/slimscroll/jquery.slimscroll.js"></script>
    <!-- main js -->
    <script src="clientdashboard/clientdashboard/assets/libs/js/main-js.js"></script>
    <!-- chart chartist js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/chartist-bundle/chartist.min.js"></script>
    <!-- sparkline js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/sparkline/jquery.sparkline.js"></script>
    <!-- morris js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/morris-bundle/raphael.min.js"></script>
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/morris-bundle/morris.js"></script>
    <!-- chart c3 js -->
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/c3charts/c3.min.js"></script>
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/c3charts/d3-5.4.0.min.js"></script>
    <script src="clientdashboard/clientdashboard/assets/vendor/charts/c3charts/C3chartjs.js"></script>
    <script src="clientdashboard/clientdashboard/assets/libs/js/dashboard-ecommerce.js"></script>
</body>
 
</html>